<script>
document.addEventListener("DOMContentLoaded", function() {
  const occField = document.querySelector("#tfa_4");
  const sectionPricing = document.getElementById("personal-accident-section-pricing");
  const sectionGeneric = document.getElementById("personal-accident-section-generic");

  const allowedTradies = ["Carpenter","Handyman","Plumber","Electrician","Landscaper"];

  // === Toggle PA sections based on occupation ===
  function togglePASections() {
    const selected = occField?.value?.trim();
    if (allowedTradies.includes(selected)) {
      sectionPricing.style.display = "none";
      sectionGeneric.style.display = "block";
    } else {
      sectionPricing.style.display = "block";
      sectionGeneric.style.display = "none";
    }
  }

  togglePASections();
  occField?.addEventListener("change", togglePASections);

  // === Follow-up sections ===
  const paFollowUp = document.getElementById("tfa_724");   // PA follow-up
  const gpFollowUp = document.getElementById("tfa_776");   // Tools follow-up
  const addFollowUp = document.getElementById("tfa_775");  // Commercial property follow-up

  // helper: toggle section visibility
  function toggleFollowUp(tileSelector, followUpEl) {
    const tile = document.querySelector(tileSelector);
    if (!tile || !followUpEl) return;
    tile.addEventListener("click", () => {
      const active = tile.classList.contains("selected");
      followUpEl.style.display = active ? "block" : "none";
    });
  }

  // Map tiles → follow-up
  toggleFollowUp('[data-field^="insurance_personal_accident_"]', paFollowUp);
  toggleFollowUp('[data-field^="insurance_general_property_"]', gpFollowUp);
  toggleFollowUp('[data-field="insurance_commercial_property"]', addFollowUp);

  // Ensure hidden by default
  if (paFollowUp) paFollowUp.style.display = "none";
  if (gpFollowUp) gpFollowUp.style.display = "none";
  if (addFollowUp) addFollowUp.style.display = "none";
});
</script>


