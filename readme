<script>
document.addEventListener("DOMContentLoaded", () => {
  function updateFinalCart() {
    // Liability
    const plOpt = document.querySelector('[data-field^="insurance_public_liability"].selected');
    document.getElementById("final-liability").textContent =
      plOpt ? plOpt.textContent.trim() : "Not selected";

    // Personal Accident
    let paText = "Not selected";
    if (document.getElementById("tfa_6354")?.checked) paText = "$1,000 p/w Personal Accident Cover";
    else if (document.getElementById("tfa_6355")?.checked) paText = "$1,500 p/w Personal Accident Cover";
    else if (document.getElementById("tfa_6412")?.checked) paText = "Quote Requested";
    else if (document.getElementById("tfa_6391")?.checked) paText = "Not Required";
    document.getElementById("final-pa").textContent = paText;

    // Tools
    let toolsText = "Not selected";
    if (document.getElementById("tfa_6362")?.checked) toolsText = "$5,000 Tools Cover";
    else if (document.getElementById("tfa_6363")?.checked) toolsText = "$7,500 Tools Cover";
    else if (document.getElementById("tfa_6392")?.checked) toolsText = "Not Required";
    document.getElementById("final-tools").textContent = toolsText;

    // Additional Covers
    const extras = [];
    if (document.getElementById("tfa_6409")?.checked) extras.push("Commercial Motor");
    if (document.getElementById("tfa_6411")?.checked) extras.push("Transit");
    if (document.getElementById("tfa_6410")?.checked) extras.push("Commercial Property");
    document.getElementById("final-extras").textContent = extras.length ? extras.join(", ") : "None";

    // Payment + Start Date → only if instant price exists
    const hasPrice = document.getElementById("tfa_3892")?.value ||
                     document.getElementById("tfa_3894")?.value ||
                     document.getElementById("tfa_3896")?.value;

    const payRow = document.getElementById("final-payment-row");
    const startRow = document.getElementById("final-start-row");

    if (hasPrice) {
      const payField = document.querySelector('input["tfa_6329"]:checked');
      document.getElementById("final-payment").textContent = payField ? payField.value : "Not selected";
      const startDateVal = document.getElementById("tfa_6328")?.value || "";
      document.getElementById("final-start").textContent = startDateVal || "Not selected";
      payRow.style.display = "";
      startRow.style.display = "";
    } else {
      payRow.style.display = "none";
      startRow.style.display = "none";
    }
  }

  // Update on load + on input change
  updateFinalCart();
  document.querySelectorAll("input, select").forEach(el => {
    el.addEventListener("change", updateFinalCart);
  });
});
</script>
