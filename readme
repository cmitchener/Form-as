  /* === Instant Pricing Calculation === */
  function calculateInstantPricing() {
    const occupation = document.querySelector('#tfa_4')?.value?.trim();
    const state = document.querySelector('#tfa_3883')?.selectedOptions[0]?.text.trim();
    const turnoverRaw = document.querySelector('#tfa_68')?.value?.trim();
    const employeesRaw = document.querySelector('#tfa_72')?.value?.trim();
    const subbieRaw = document.querySelector('#tfa_3990')?.value?.trim();

    // Turnover
    const turnoverVal = parseFloat((turnoverRaw || "").replace(/[^0-9.]/g, ''));
    let turnover = "";
    if (!isNaN(turnoverVal)) {
      if (turnoverVal <= 100000) turnover = "Up to $100k";
      else if (turnoverVal <= 250000) turnover = "Up to $250k";
      else turnover = "Up to $500k";
    }

    // Employees
    const employeesVal = parseInt((employeesRaw || "").replace(/[^0-9]/g, ''), 10);
    let employees = employeesVal <= 2 ? "1-2" : employeesVal <= 4 ? "3-4" : "5+";

    // Subbies
    const subbieVal = parseFloat((subbieRaw || "").replace(/[^0-9.]/g, ''));
    let subcontractors = "No", subbieAmount = "None";
    if (!isNaN(subbieVal) && subbieVal > 0) {
      subcontractors = "Yes";
      if (subbieVal < 50000) subbieAmount = "Under $50k";
      else if (subbieVal <= 100000) subbieAmount = "$50k to $100k";
      else subbieAmount = "Over $100k";
    }

    const key = `${occupation}-${state}-${turnover}-${subcontractors}-${employees}-${subbieAmount}`;
    const result = pricingTable[key];

    const label5 = document.querySelector('#label5');
    const label10 = document.querySelector('#label10');
    const label20 = document.querySelector('#label20');

    if (result) {
      const monthly5 = Math.round(result["5M"] / 8 * 1.1);
      const monthly10 = Math.round(result["10M"] / 8 * 1.1);
      const monthly20 = Math.round(result["20M"] / 8 * 1.1);

       label5.innerHTML = `<div class="cover-option"><div class="cover-heading">$5M Liability Cover</div><div class="cover-price"><strong>$${result["5M"]}</strong> annually</div><div class="cover-price-alt">or <strong>$${monthly5}</strong> × 8 monthly</div><div class="option-cta add">Add Cover</div></div>`;
    label10.innerHTML = `<div class="cover-option"><div class="cover-heading">$10M Liability Cover</div><div class="cover-price"><strong>$${result["10M"]}</strong> annually</div><div class="cover-price-alt">or <strong>$${monthly10}</strong> × 8 monthly</div><div class="option-cta add">Add Cover</div></div>`;
    label20.innerHTML = `<div class="cover-option"><div class="cover-heading">$20M Liability Cover</div><div class="cover-price"><strong>$${result["20M"]}</strong> annually</div><div class="cover-price-alt">or <strong>$${monthly20}</strong> × 8 monthly</div><div class="option-cta add">Add Cover</div></div>`;
  
      document.getElementById("tfa_3892").value = result["5M"];
      document.getElementById("tfa_3894").value = result["10M"];
      document.getElementById("tfa_3896").value = result["20M"];
      document.getElementById("tfa_6269").value = monthly5;
document.getElementById("tfa_6271").value = monthly10;
document.getElementById("tfa_6273").value = monthly20;
      document.getElementById("tfa_3879").value = Yes;
    } else {
    label5.innerHTML = `$5M Public Liability Cover<div class="option-cta request">Request Quote</div>`;
    label10.innerHTML = `$10M Public Liability Cover<div class="option-cta request">Request Quote</div>`;
    label20.innerHTML = `$20M Public Liability Cover<div class="option-cta request">Request Quote</div>`;
  }

    document.getElementById("instant-pricing-container").style.display = "block";
    document.getElementById("instant-price-btn").style.display = "none";
  }

  document.getElementById("instant-price-btn")?.addEventListener("click", calculateInstantPricing);

  /* === Liability Option Click Sync Radios === */
  document.querySelectorAll('#liability-limit-options .option').forEach(opt => {
    opt.addEventListener('click', () => {
      document.querySelectorAll('#liability-limit-options .option').forEach(o => o.classList.remove('selected'));
      opt.classList.add('selected');
      const plField = document.getElementById("tfa_3911");
      if (plField) plField.value = opt.getAttribute("data-field");
      setTimeout(updateQuoteSummary, 20);
    });
  });

  /* === Personal Accident Tiles + Radios === */
  const radio1000 = document.getElementById("tfa_6354");
  const radio1500 = document.getElementById("tfa_6355");
  const paOptions = document.querySelectorAll('[data-field^="insurance_personal_accident_"]');
  const summaryPA = document.getElementById("summary-pa");

  function applyPASel(val) {
    paOptions.forEach(o => o.classList.remove("selected"));
    if (radio1000) radio1000.checked = false;
    if (radio1500) radio1500.checked = false;

    if (!val) { summaryPA.textContent = "Not selected"; return; }

    const tile = document.querySelector(`[data-field="insurance_personal_accident_${val}"]`);
    if (tile) tile.classList.add("selected");
    if (val === "1000" && radio1000) radio1000.checked = true;
    if (val === "1500" && radio1500) radio1500.checked = true;
    summaryPA.textContent = val === "1000" ? "$1,000 Personal Accident Cover" : "$1,500 Personal Accident Cover";
    setTimeout(updateQuoteSummary, 20);
  }

  paOptions.forEach(opt => {
    opt.addEventListener("click", () => {
      const val = opt.getAttribute("data-field").split("_").pop();
      const isSel = opt.classList.contains("selected");
      applyPASel(isSel ? "" : val);
    });
  });

  radio1000?.addEventListener("change", () => { if (radio1000.checked) applyPASel("1000"); });
  radio1500?.addEventListener("change", () => { if (radio1500.checked) applyPASel("1500"); });

 
</script>

<script>
document.addEventListener("DOMContentLoaded", () => {
  const liabTiles = document.querySelectorAll('#liability-limit-options .option');
  const liabWarning = document.getElementById("liability-warning");
  const summaryLiability = document.getElementById("summary-liability");

  const radio5 = document.getElementById("tfa_6346");
  const radio10 = document.getElementById("tfa_6347");
  const radio20 = document.getElementById("tfa_6348");

  function applyLiability(val) {
    // clear tile selection
    liabTiles.forEach(o => o.classList.remove("selected"));
    [radio5, radio10, radio20].forEach(r => r && (r.checked = false));

    if (!val) {
      summaryLiability.textContent = "Not selected";
      liabWarning.style.display = "block";
      return;
    }

    const tile = document.querySelector(`[data-field="insurance_public_liability${val.toLowerCase()}"]`);
    if (tile) tile.classList.add("selected");

    if (val === "5m" && radio5) radio5.checked = true;
    if (val === "10m" && radio10) radio10.checked = true;
    if (val === "20m" && radio20) radio20.checked = true;

    summaryLiability.textContent = `$${val.toUpperCase()} Public Liability Cover`;
    liabWarning.style.display = "none";
    setTimeout(updateQuoteSummary, 20);
  }

  // Tile click
  liabTiles.forEach(opt => {
    opt.addEventListener('click', () => {
      const val = opt.getAttribute("data-field").replace("insurance_public_liability", "");
      const isSel = opt.classList.contains("selected");
      applyLiability(isSel ? "" : val);
    });
  });

  // Radio change → update tiles
  [radio5, radio10, radio20].forEach(r => {
    r?.addEventListener("change", () => {
      if (r.checked) {
        if (r === radio5) applyLiability("5m");
        if (r === radio10) applyLiability("10m");
        if (r === radio20) applyLiability("20m");
      }
    });
  });

  // Validate on Next button
  const nextBtn = document.querySelector('.wfPageNextButton');
  if (nextBtn) {
    nextBtn.addEventListener('click', (e) => {
      if (![radio5, radio10, radio20].some(r => r?.checked)) {
        e.preventDefault();
        liabWarning.style.display = "block";
        window.scrollTo({ top: document.getElementById("quote-summary").offsetTop - 50, behavior: "smooth" });
      }
    });
  }
});
</script>

<script>
document.addEventListener("DOMContentLoaded", () => {
  // Map extras to their hidden inputs
  const extrasMap = {
    "insurance_transit": "tfa_6405",
    "insurance_commercial_motor": "tfa_6403",
    "insurance_commercial_property": "tfa_6404"
  };

  const multiPicklist = document.getElementById("tfa_6402"); // master multi-picklist field

  Object.entries(extrasMap).forEach(([field, hiddenId]) => {
    const tile = document.querySelector(`[data-field="${field}"]`);
    const hidden = document.getElementById(hiddenId);
    if (!tile || !hidden) return;

    tile.addEventListener("click", () => {
      const active = tile.classList.toggle("selected");
      hidden.value = active ? "Yes" : "";

      // Build combined list for tfa_6402
      const selectedExtras = Object.entries(extrasMap)
        .filter(([f, id]) => document.getElementById(id)?.value === "Yes")
        .map(([f]) => {
          if (f === "insurance_transit") return "Transit";
          if (f === "insurance_commercial_motor") return "Commercial Motor";
          if (f === "insurance_commercial_property") return "Commercial Property";
          return "";
        })
        .filter(Boolean);

      if (multiPicklist) {
        multiPicklist.value = selectedExtras.join(", ");
      }

      setTimeout(() => window.updateQuoteSummary?.(), 20);
    });
  });
});
</script>


<script>
document.addEventListener("DOMContentLoaded", function() {
  const occField = document.querySelector("#tfa_4");
  const sectionPricing = document.getElementById("personal-accident-section-pricing");
  const sectionGeneric = document.getElementById("personal-accident-section-generic");

  const paRequestTile = document.querySelector('[data-field="insurance_personal_accident_request"]');
  const paNotReqTile = document.querySelector('[data-field="insurance_personal_accident_notrequired"]');
  const paRadioHidden = document.getElementById("tfa_6412"); // hidden radio for generic PA

  // === Allowed tradie occupations for pricing ===
  const tradieOccs = ["Carpenter","Handyman","Plumber","Electrician","Landscaper"];

  // === Toggle PA sections based on occupation ===
  function togglePASections() {
    const selected = occField?.value?.trim();
    if (tradieOccs.includes(selected)) {
      // show pricing for tradies
      sectionPricing.style.display = "block";
      sectionGeneric.style.display = "none";
    } else {
      // show generic for non-tradies
      sectionPricing.style.display = "none";
      sectionGeneric.style.display = "block";
    }
  }

  togglePASections();
  occField?.addEventListener("change", togglePASections);

  // === Generic PA selection → update hidden field ===
  if (paRequestTile) {
    paRequestTile.addEventListener("click", () => {
      document.querySelectorAll('#personal-accident-section-generic .option').forEach(o => o.classList.remove("selected"));
      paRequestTile.classList.add("selected");
      if (paRadioHidden) paRadioHidden.value = "Quote Requested";
    });
  }

  if (paNotReqTile) {
    paNotReqTile.addEventListener("click", () => {
      document.querySelectorAll('#personal-accident-section-generic .option').forEach(o => o.classList.remove("selected"));
      paNotReqTile.classList.add("selected");
      if (paRadioHidden) paRadioHidden.value = "";
    });
  }
});
</script>


