document.addEventListener("DOMContentLoaded", () => {
  /* ---------------- PERSONAL ACCIDENT ---------------- */
  const paMap = {
    "insurance_personal_accident_1000": "tfa_6354",
    "insurance_personal_accident_1500": "tfa_6355",
    "insurance_personal_accident_request": "tfa_6412",
    "insurance_personal_accident_notrequired": "tfa_6391"
  };
  const paFollowUp = document.getElementById("tfa_724");

  Object.entries(paMap).forEach(([dataField, radioId]) => {
    const el = document.querySelector(`[data-field="${dataField}"]`);
    const input = document.getElementById(radioId);
    if (!el || !input) return;

    el.addEventListener("click", () => {
      // deselect all PA
      Object.keys(paMap).forEach(df => {
        const other = document.querySelector(`[data-field="${df}"]`);
        if (other) other.classList.remove("selected");
        const otherInput = document.getElementById(paMap[df]);
        if (otherInput) otherInput.checked = false;
      });
      // select clicked
      el.classList.add("selected");
      input.checked = true;
      if (paFollowUp) paFollowUp.style.display = dataField.includes("notrequired") ? "none" : "block";
    });

    // Restore state
    if (input.checked) {
      el.classList.add("selected");
      if (paFollowUp) paFollowUp.style.display = dataField.includes("notrequired") ? "none" : "block";
    }
  });

  /* ---------------- TOOLS INSURANCE ---------------- */
  const toolsMap = {
    "insurance_general_property_5000": "tfa_6362",
    "insurance_general_property_7500": "tfa_6363",
    "insurance_general_property_notrequired": "tfa_6392"
  };
  const toolsFollowUp = document.getElementById("tfa_776");

  Object.entries(toolsMap).forEach(([dataField, radioId]) => {
    const el = document.querySelector(`[data-field="${dataField}"]`);
    const input = document.getElementById(radioId);
    if (!el || !input) return;

    el.addEventListener("click", () => {
      // deselect all Tools
      Object.keys(toolsMap).forEach(df => {
        const other = document.querySelector(`[data-field="${df}"]`);
        if (other) other.classList.remove("selected");
        const otherInput = document.getElementById(toolsMap[df]);
        if (otherInput) otherInput.checked = false;
      });
      el.classList.add("selected");
      input.checked = true;
      if (toolsFollowUp) toolsFollowUp.style.display = dataField.includes("notrequired") ? "none" : "block";
    });

    // Restore state
    if (input.checked) {
      el.classList.add("selected");
      if (toolsFollowUp) toolsFollowUp.style.display = dataField.includes("notrequired") ? "none" : "block";
    }
  });

  /* ---------------- ADDITIONAL INSURANCES ---------------- */
  const extrasMap = {
    "insurance_commercial_motor": "tfa_6409",
    "insurance_transit": "tfa_6411",
    "insurance_commercial_property": "tfa_6410"
  };
  const propFollowUp = document.getElementById("tfa_778");

  Object.entries(extrasMap).forEach(([dataField, checkboxId]) => {
    const el = document.querySelector(`[data-field="${dataField}"]`);
    const input = document.getElementById(checkboxId);
    if (!el || !input) return;

    el.addEventListener("click", () => {
      const isSelected = el.classList.toggle("selected");
      input.checked = isSelected;
      if (dataField === "insurance_commercial_property" && propFollowUp) {
        propFollowUp.style.display = isSelected ? "block" : "none";
      }
    });

    // Restore state
    if (input.checked) {
      el.classList.add("selected");
      if (dataField === "insurance_commercial_property" && propFollowUp) {
        propFollowUp.style.display = "block";
      }
    }
  });

  // Hide Commercial Property follow-up if not selected
  if (propFollowUp && !document.getElementById("tfa_6410")?.checked) {
    propFollowUp.style.display = "none";
  }
});
