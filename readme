  function calculateInstantPricing() {
    const occ = document.querySelector('#tfa_4')?.value?.trim();
    const state = document.querySelector('#tfa_3883')?.selectedOptions[0]?.text.trim();
    const turnoverRaw  = document.querySelector('#tfa_68')?.value || "";
    const employeesRaw = document.querySelector('#tfa_72')?.value || "";
    const subbieRaw    = document.querySelector('#tfa_3990')?.value || "";

    // Turnover → range
    const tVal = parseFloat(turnoverRaw.replace(/[^0-9.]/g, ''));
    let turnover = "";
    if (!isNaN(tVal)) {
      if (tVal <= 100000) turnover = "Up to $100k";
      else if (tVal <= 250000) turnover = "Up to $250k";
      else turnover = "Up to $500k";
    }

    // Employees → bucket
    const eVal = parseInt(employeesRaw.replace(/[^0-9]/g, ''), 10);
    let employees = (eVal || 0) <= 2 ? "1-2" : eVal <= 4 ? "3-4" : "5+";

    // Subbies → Yes/No + band
    const sVal = parseFloat(subbieRaw.replace(/[^0-9.]/g, ''));
    let subcontractors = "No", subbieAmount = "None";
    if (!isNaN(sVal) && sVal > 0) {
      subcontractors = "Yes";
      if (sVal < 50000) subbieAmount = "Under $50k";
      else if (sVal <= 100000) subbieAmount = "$50k to $100k";
      else subbieAmount = "Over $100k";
    }

    const key = `${occ}-${state}-${turnover}-${subcontractors}-${employees}-${subbieAmount}`;
    const result = pricingTable[key];

    const label5  = document.getElementById("label5");
    const label10 = document.getElementById("label10");
    const label20 = document.getElementById("label20");

    if (result) {
      const m5  = Math.round(result["5M"]  / 8 * 1.1);
      const m10 = Math.round(result["10M"] / 8 * 1.1);
      const m20 = Math.round(result["20M"] / 8 * 1.1);

      label5.innerHTML  = `<div><div class="cover-heading">Add $5M Liability Cover</div><div><strong>$${result["5M"]}</strong> annually</div><div>or <strong>$${m5}</strong> × 8 monthly</div></div>`;
      label10.innerHTML = `<div><div class="cover-heading">Add $10M Liability Cover</div><div><strong>$${result["10M"]}</strong> annually</div><div>or <strong>$${m10}</strong> × 8 monthly</div></div>`;
      label20.innerHTML = `<div><div class="cover-heading">Add $20M Liability Cover</div><div><strong>$${result["20M"]}</strong> annually</div><div>or <strong>$${m20}</strong> × 8 monthly</div></div>`;

      // save to hidden fields
      const f5  = document.getElementById("tfa_3892");
      const f10 = document.getElementById("tfa_3894");
      const f20 = document.getElementById("tfa_3896");
            document.getElementById("tfa_3879").value = "Yes";

    document.getElementById("tfa_6269").value = monthly5;
document.getElementById("tfa_6271").value = monthly10;
document.getElementById("tfa_6273").value = monthly20;  
      
      if (f5)  f5.value  = result["5M"];
      if (f10) f10.value = result["10M"];
      if (f20) f20.value = result["20M"];
    } else {
      label5.textContent  = "Request Quote – $5M Public Liability Cover";
      label10.textContent = "Request Quote – $10M Public Liability Cover";
      label20.textContent = "Request Quote – $20M Public Liability Cover";

        document.getElementById("tfa_3879").value = "No";
      if (fallbackCta) fallbackCta.style.display = "block";
    }
      
      // clear hidden price fields
      ["tfa_3892","tfa_3894","tfa_3896"].forEach(id => { const el = document.getElementById(id); if (el) el.value = ""; });
    }

    updateSummaryAndCart();
  }

  // Recalc when the upstream inputs change
  ["#tfa_4","#tfa_3883","#tfa_68","#tfa_72","#tfa_3990"].forEach(sel=>{
    document.querySelector(sel)?.addEventListener("change", calculateInstantPricing);
  });

  /* ---------- Personal Accident visibility (priced vs generic) ---------- */
  function togglePersonalAccident() {
    const occ = (document.querySelector('#tfa_4')?.value || "").trim().toLowerCase();
    const priced = ["carpenter","landscaper","electrician","plumber","handyman"];
    const pricedEl  = document.getElementById("personal-accident-section-pricing");
    const genericEl = document.getElementById("personal-accident-section-generic");
    if (!pricedEl || !genericEl) return;
    if (priced.includes(occ)) {
      pricedEl.style.display = "block";
      genericEl.style.display = "none";
    } else {
      pricedEl.style.display = "none";
      genericEl.style.display = "block";
    }
  }
  document.getElementById("tfa_4")?.addEventListener("change", togglePersonalAccident);

  /* ---------- Tile click handlers + radio sync ---------- */
  function selectInGroup(opt) {
    const group = opt.closest(".option-group");
    if (group) group.querySelectorAll(".option").forEach(o => o.classList.remove("selected"));
    opt.classList.add("selected");
  }

  document.querySelectorAll(".option").forEach(opt => {
    opt.addEventListener("click", () => {
      const key = opt.getAttribute("data-field") || "";
      const wasSelected = opt.classList.contains("selected");

      // single-select groups
      if (/^insurance_public_liability/.test(key) ||
          /^insurance_personal_accident_/.test(key) ||
          /^insurance_general_property_/.test(key)) {
        selectInGroup(opt);
      } else {
        // extras = toggles
        opt.classList.toggle("selected");
      }

      // Map to radios
      if (/^insurance_public_liability/.test(key)) {
        const r5  = document.getElementById("tfa_6346");
        const r10 = document.getElementById("tfa_6347");
        const r20 = document.getElementById("tfa_6348");
        if (r5)  r5.checked  = /5m$/.test(key);
        if (r10) r10.checked = /10m$/.test(key);
        if (r20) r20.checked = /20m$/.test(key);
      }

      if (/^insurance_personal_accident_/.test(key)) {
        const r1000   = document.getElementById("tfa_6354");
        const r1500   = document.getElementById("tfa_6355");
        const rReq    = document.getElementById("tfa_6412");
        const rNotReq = document.getElementById("tfa_6391");
        if (r1000)   r1000.checked   = /_1000$/.test(key);
        if (r1500)   r1500.checked   = /_1500$/.test(key);
        if (rReq)    rReq.checked    = /_request$/.test(key);
        if (rNotReq) rNotReq.checked = /_notrequired$/.test(key);
      }

      if (/^insurance_general_property_/.test(key)) {
        const r5k   = document.getElementById("tfa_6362");
        const r75k  = document.getElementById("tfa_6363");
        const rNone = document.getElementById("tfa_6392");
        if (r5k)   r5k.checked   = /_5000$/.test(key);
        if (r75k)  r75k.checked  = /_7500$/.test(key);
        if (rNone) rNone.checked = /_notrequired$/.test(key);
      }

      // Additional covers (tiles toggle the YES radio of each)
      if (key === "insurance_commercial_motor") {
        const yes = document.getElementById("tfa_6409"); // YES radio
        if (yes) yes.checked = opt.classList.contains("selected");
      }
      if (key === "insurance_transit") {
        const yes = document.getElementById("tfa_6411"); // YES radio
        if (yes) yes.checked = opt.classList.contains("selected");
      }
      if (key === "insurance_commercial_property") {
        const yes = document.getElementById("tfa_6410"); // YES radio
        if (yes) yes.checked = opt.classList.contains("selected");
      }

</script>
