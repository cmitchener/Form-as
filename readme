// ========== CART BUILDER ==========
// This should be placed after your <table id="cart-summary"> in your HTML
// and after all the "tile selection" logic in your <script>

function updateSummaryAndCart() {
  const body = document.getElementById("cart-body");
  body.innerHTML = "";

  // === Liability ===
  const liab = document.querySelector('#liability-limit-options .option.selected');
  if (liab) {
    const cover = liab.textContent.trim();
    let ann="Quote Requested", mon="";
    if (cover.includes("5M") && document.getElementById("tfa_3892").value) {
      ann = `$${document.getElementById("tfa_3892").value}`;
      mon = `$${document.getElementById("tfa_6269").value} × 8`;
    }
    if (cover.includes("10M") && document.getElementById("tfa_3894").value) {
      ann = `$${document.getElementById("tfa_3894").value}`;
      mon = `$${document.getElementById("tfa_6271").value} × 8`;
    }
    if (cover.includes("20M") && document.getElementById("tfa_3896").value) {
      ann = `$${document.getElementById("tfa_3896").value}`;
      mon = `$${document.getElementById("tfa_6273").value} × 8`;
    }
    body.insertAdjacentHTML("beforeend", `
      <tr>
        <td style="padding:8px; border:1px solid #ddd;">Public Liability</td>
        <td style="padding:8px; border:1px solid #ddd;">${cover}</td>
        <td style="padding:8px; border:1px solid #ddd;">${ann}</td>
        <td style="padding:8px; border:1px solid #ddd;">${mon}</td>
      </tr>
    `);
  }

  // === Personal Accident ===
  const pa = document.querySelector('#personal-accident-section-pricing .option.selected, #personal-accident-section-generic .option.selected');
  if (pa) {
    const cover = pa.textContent.trim();
    let ann = "", mon = "";
    if (cover.includes("1,000")) {
      ann = "$1,030";
      mon = "$142 × 8";
    }
    if (cover.includes("1,500")) {
      ann = "$1,555";
      mon = "$214 × 8";
    }
    // For "Not Required" or "Request" just show the name, no price
    body.insertAdjacentHTML("beforeend", `
      <tr>
        <td style="padding:8px; border:1px solid #ddd;">Personal Accident</td>
        <td style="padding:8px; border:1px solid #ddd;">${cover}</td>
        <td style="padding:8px; border:1px solid #ddd;">${ann}</td>
        <td style="padding:8px; border:1px solid #ddd;">${mon}</td>
      </tr>
    `);
  }

  // === General Property/Tools ===
  const tools = document.querySelector('#tools-section .option.selected');
  if (tools) {
    const cover = tools.textContent.trim();
    let ann = "", mon = "";
    if (cover.includes("5,000")) {
      ann = "$455";
      mon = "$63 × 8";
    }
    if (cover.includes("7,500")) {
      ann = "$585";
      mon = "$81 × 8";
    }
    body.insertAdjacentHTML("beforeend", `
      <tr>
        <td style="padding:8px; border:1px solid #ddd;">General Property (Tools)</td>
        <td style="padding:8px; border:1px solid #ddd;">${cover}</td>
        <td style="padding:8px; border:1px solid #ddd;">${ann}</td>
        <td style="padding:8px; border:1px solid #ddd;">${mon}</td>
      </tr>
    `);
  }

  // === Additional Insurances (Extras) ===
  const extras = [];
  document.querySelectorAll('#extras-section .option.selected').forEach(opt => {
    extras.push(opt.textContent.trim());
  });
  if (extras.length) {
    body.insertAdjacentHTML("beforeend", `
      <tr>
        <td style="padding:8px; border:1px solid #ddd;">Additional Covers</td>
        <td style="padding:8px; border:1px solid #ddd;">${extras.join(", ")}</td>
        <td style="padding:8px; border:1px solid #ddd;">--</td>
        <td style="padding:8px; border:1px solid #ddd;">--</td>
      </tr>
    `);
  }
}

// ========== TILE SELECTION LOGIC (refactored) ==========
document.addEventListener("DOMContentLoaded", function() {
  // Liability, PA, Tools
  [
    "#liability-limit-options .option",
    "#personal-accident-section-pricing .option",
    "#personal-accident-section-generic .option",
    "#tools-section .option",
    "#extras-section .option"
  ].forEach(sel => {
    document.querySelectorAll(sel).forEach(opt => {
      opt.addEventListener("click", function() {
        // Unselect all siblings in its group except 'extras'
        if (!opt.closest("#extras-section")) {
          opt.closest(".option-group").querySelectorAll(".option").forEach(o => o.classList.remove("selected"));
          opt.classList.add("selected");
        } else {
          opt.classList.toggle("selected");
        }
        updateSummaryAndCart();
      });
    });
  });

  // Initial cart build
  updateSummaryAndCart();
});
</script>
