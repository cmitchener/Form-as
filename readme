<script>
document.addEventListener("DOMContentLoaded", function() {
  // === Helper: update summary status ===
  function setCartStatus(elId, status) {
    const el = document.getElementById(elId);
    if (!el) return;
    if (status === "selected") el.innerHTML = '<span class="status selected">Selected</span>';
    else if (status === "notselected") el.innerHTML = '<span class="status notselected">Not selected</span>';
    else if (status === "none") el.innerHTML = '<span class="status none">None</span>';
    else el.innerHTML = '<span class="status notselected">Not selected</span>';
  }

  // === Elements ===
  const occField = document.querySelector("#tfa_4");
  const stateField = document.querySelector("#tfa_3883");
  const turnoverField = document.querySelector("#tfa_68");
  const employeesField = document.querySelector("#tfa_72");
  const subbieAmtField = document.querySelector("#tfa_3990");

  const paPricing = document.getElementById("personal-accident-section-pricing");
  const paGeneric = document.getElementById("personal-accident-section-");
  const paFollowUp = document.getElementById("tfa_724");
  const gpFollowUp = document.getElementById("tfa_776");
  const addFollowUp = document.getElementById("tfa_775");

  const tradies = ["Carpenter","Handyman","Plumber","Electrician","Landscaper"];

  // === Toggle PA sections & reset ===
  function togglePASections() {
    const occ = occField?.value?.trim();
    if (tradies.includes(occ)) {
      paPricing.style.display = "none";
      paGeneric.style.display = "block";
    } else {
      paPricing.style.display = "block";
      paGeneric.style.display = "none";
    }
    if (paFollowUp) paFollowUp.style.display = "none";
    setCartStatus("cart-pa","notselected");
  }

  // === Follow-up mapping ===
  function bindFollowUp(tileSelector, followUpEl, cartElId, notReqSelector=null) {
    const tiles = document.querySelectorAll(tileSelector);
    tiles.forEach(tile => {
      tile.addEventListener("click", () => {
        const active = tile.classList.contains("selected");
        if (followUpEl) followUpEl.style.display = active ? "block" : "none";
        if (cartElId) setCartStatus(cartElId, active ? "selected" : "notselected");
      });
    });
    if (notReqSelector) {
      const notReq = document.querySelector(notReqSelector);
      notReq?.addEventListener("click", () => {
        if (followUpEl) followUpEl.style.display = "none";
        if (cartElId) setCartStatus(cartElId, "notselected");
      });
    }
    if (followUpEl) followUpEl.style.display = "none";
  }

  bindFollowUp('[data-field^="insurance_personal_accident_"]', paFollowUp, "cart-pa", '[data-field="insurance_personal_accident_notrequired"]');
  bindFollowUp('[data-field^="insurance_general_property_"]', gpFollowUp, "cart-tools", '[data-field="insurance_general_property_notrequired"]');
  bindFollowUp('[data-field="insurance_commercial_property"]', addFollowUp, "cart-extras", '[data-field="insurance_commercial_property_notrequired"]');

  
  // === Liability selection → summary ===
  document.querySelectorAll('#liability-limit-options .option').forEach(tile => {
    tile.addEventListener("click", () => {
      const active = tile.classList.contains("selected");
      setCartStatus("cart-liability", active ? "selected" : "notselected");
    });
  });

  // === Init ===
  togglePASections();
  occField?.addEventListener("change", togglePASections);

  setCartStatus("cart-liability","notselected");
  setCartStatus("cart-pa","notselected");
  setCartStatus("cart-tools","notselected");
  setCartStatus("cart-extras","none");
  setCartStatus("cart-payment","notselected");
  setCartStatus("cart-start","notselected");
});
</script>


