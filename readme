  function calculateInstantPricing() {
    const occupation = document.querySelector('#tfa_4')?.value?.trim();
    const state = document.querySelector('#tfa_3883')?.selectedOptions[0]?.text.trim();
    const turnoverRaw = document.querySelector('#tfa_68')?.value?.trim();
    const employeesRaw = document.querySelector('#tfa_72')?.value?.trim();
    const subbieRaw = document.querySelector('#tfa_3990')?.value?.trim();

    // Turnover
    const turnoverVal = parseFloat((turnoverRaw || "").replace(/[^0-9.]/g,''));
    let turnover = "";
    if (!isNaN(turnoverVal)) {
      if (turnoverVal <= 100000) turnover = "Up to $100k";
      else if (turnoverVal <= 250000) turnover = "Up to $250k";
      else turnover = "Up to $500k";
    }

    // Employees
    const employeesVal = parseInt((employeesRaw || "").replace(/[^0-9]/g,''),10);
    let employees = employeesVal <= 2 ? "1-2" : employeesVal <= 4 ? "3-4" : "5+";

    // Subbies
    const subbieVal = parseFloat((subbieRaw || "").replace(/[^0-9.]/g,''));
    let subcontractors="No", subbieAmount="None";
    if (!isNaN(subbieVal) && subbieVal>0) {
      subcontractors="Yes";
      if (subbieVal<50000) subbieAmount="Under $50k";
      else if (subbieVal<=100000) subbieAmount="$50k to $100k";
      else subbieAmount="Over $100k";
    }

    const key = `${occupation}-${state}-${turnover}-${subcontractors}-${employees}-${subbieAmount}`;
    const result = pricingTable[key];

    const label5=document.getElementById("label5"),
          label10=document.getElementById("label10"),
          label20=document.getElementById("label20");

    if(result){
      const monthly5=Math.round(result["5M"]/8*1.1);
      const monthly10=Math.round(result["10M"]/8*1.1);
      const monthly20=Math.round(result["20M"]/8*1.1);

      label5.textContent=`Add $5M Liability Cover – $${result["5M"]} annually / $${monthly5} ×8 monthly`;
      label10.textContent=`Add $10M Liability Cover – $${result["10M"]} annually / $${monthly10} ×8 monthly`;
      label20.textContent=`Add $20M Liability Cover – $${result["20M"]} annually / $${monthly20} ×8 monthly`;

      document.getElementById("tfa_3892").value=result["5M"];
      document.getElementById("tfa_3894").value=result["10M"];
      document.getElementById("tfa_3896").value=result["20M"];
      document.getElementById("tfa_6269").value = monthly5;
document.getElementById("tfa_6271").value = monthly10;
document.getElementById("tfa_6273").value = monthly20;
      document.getElementById("tfa_3879").value = Yes;
    } else {
      label5.textContent="Request Quote – $5M Public Liability Cover";
      label10.textContent="Request Quote – $10M Public Liability Cover";
      label20.textContent="Request Quote – $20M Public Liability Cover";
    }

    updateSummaryAndCart();
  }
  ["#tfa_4","#tfa_3883","#tfa_68","#tfa_72","#tfa_3990"].forEach(sel=>{
    document.querySelector(sel)?.addEventListener("change",calculateInstantPricing);
  });
  setTimeout(calculateInstantPricing,400);

  /* ========== TILE SELECTION LOGIC ========== */
  function setupTile(groupSelector,mapping){
    document.querySelectorAll(groupSelector+" .option").forEach(opt=>{
      opt.addEventListener("click",()=>{
        if(!opt.closest(".option-group")) return;
        opt.closest(".option-group").querySelectorAll(".option").forEach(o=>o.classList.remove("selected"));
        opt.classList.add("selected");
        Object.entries(mapping).forEach(([key,id])=>{
          document.getElementById(id).checked = opt.getAttribute("data-field").includes(key);
        });
        updateSummaryAndCart();
      });
    });
  }

  // Liability radios
  setupTile("#liability-limit-options",{"5m":"tfa_6346","10m":"tfa_6347","20m":"tfa_6348"});

  // Personal Accident radios
  setupTile("#personal-accident-section-pricing",{"1000":"tfa_6354","1500":"tfa_6355","notrequired":"tfa_6391"});
  setupTile("#personal-accident-section-generic",{"request":"tfa_6412","notrequired":"tfa_6391"});

  // Tools radios
  setupTile("#tools-section",{"5000":"tfa_6362","7500":"tfa_6363","notrequired":"tfa_6392"});

  // Extras multipicklist
  const multi=document.getElementById("tfa_6402");
  document.querySelectorAll('#extras-section .option').forEach(opt=>{
    opt.addEventListener("click",()=>{
      opt.classList.toggle("selected");
      const val=opt.textContent.trim();
      [...multi.options].forEach(o=>{
        if(o.text===val) o.selected=opt.classList.contains("selected");
      });
      updateSummaryAndCart();
    });
  });

  /* ========== PA Generic vs Priced ========== */
  function togglePASections(){
    const occ=document.querySelector("#tfa_4")?.value?.trim().toLowerCase();
    const priced=["carpenter","landscaper","electrician","plumber"];
    document.getElementById("personal-accident-section-pricing").style.display = priced.includes(occ)?"block":"none";
    document.getElementById("personal-accident-section-generic").style.display = priced.includes(occ)?"none":"block";
  }
  document.getElementById("tfa_4")?.addEventListener("change",togglePASections);
  setTimeout(togglePASections,400);


</script>
